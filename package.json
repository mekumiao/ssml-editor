    "lodash.sorteduniqby": "^4.7.0",
    "lodash.throttle": "^4.1.1",
    "lodash.uniqby": "^4.7.0",
    "pinia": "^2.1.6",
    "pinyin-pro": "^3.16.4",
    "pinyinize": "^3.3.1",
    "snabbdom": "^3.5.1",
    "vue": "^3.3.4",
    "xml-formatter": "^3.5.0",
    "xml2js": "^0.6.2"
