    "@types/lodash.mergewith": "^4.6.7",
    "@types/lodash.sorteduniqby": "^4.7.7",
    "@types/lodash.throttle": "^4.1.7",
    "@types/lodash.uniqby": "^4.7.7",
    "@vueuse/core": "^10.4.1",
    "@wangeditor/editor": "^5.1.23",
    "bootstrap": "^5.3.2",
    "element-plus": "^2.3.12",
    "event-emitter": "^0.3.5",
    "lodash.debounce": "^4.0.8",
    "lodash.merge": "^4.6.2",
