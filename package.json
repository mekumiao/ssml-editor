{
  "name": "@mekumiao/ssml-editor",
  "version": "1.1.4",
  "private": false,
  "description": "基于wangeditor实现的支持SSML语法的编辑器",
  "keywords": [
    "SSML",
    "editor",
    "编辑器"
  ],
  "homepage": "https://github.com/mekumiao/ssml-editor/",
